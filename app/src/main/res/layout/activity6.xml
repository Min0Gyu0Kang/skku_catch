<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/a6_cancel"
        android:layout_width="366dp"
        android:layout_height="83dp"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="11dp"
        android:text="cancel"
        app:layout_constraintEnd_toStartOf="@+id/button3"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="604dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:text="Restaurant Name"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="12dp" />

    <TextView
        android:id="@+id/a6_people"
        android:layout_width="349dp"
        android:layout_height="41dp"
        android:layout_marginStart="24dp"
        android:text="People: num"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="260dp" />

    <TextView
        android:id="@+id/a6_date"
        android:layout_width="351dp"
        android:layout_height="47dp"
        android:layout_marginStart="24dp"
        android:text="Dates: 00/00/00"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="330dp" />

    <TextView
        android:id="@+id/a6_time"
        android:layout_width="355dp"
        android:layout_height="31dp"
        android:layout_marginStart="24dp"
        android:text="Times: 00:00"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="454dp" />

    <ImageView
        android:id="@+id/a6_figure"
        android:layout_width="349dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/ic_launcher_background"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="63dp" />

    <TextView
        android:id="@+id/a6_reserve_title"
        android:layout_width="341dp"
        android:layout_height="51dp"
        android:layout_marginStart="24dp"
        android:text="My Reservation"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="182dp" />

</androidx.constraintlayout.widget.ConstraintLayout>